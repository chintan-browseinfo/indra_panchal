<?xml version="1.0" encoding="utf-8"?>
<odoo>  
	<record id="view_univercity_course_tree" model="ir.ui.view">
		<field name="name">univercitycourse.module.tree</field>
		<field name="model">univercitycourse.module</field>
		<field name="priority">3</field>
		<field name="arch" type="xml">
			<tree string="Univercity Course">
				<field name="name" String="Course Name" />
				<field name="code" String="Course Code" />
				<field name="course_type" String="Course Type" />
				<field name="course_name" String="Course Full Name" />
				<field name="education_id" String="Education Stream" />
				<field name="course_duration" String="Course Duration" />
				<field name="qualification_id" String="Required Qualification" invisible="1"   />
				<field name="course_fees" String="Course Fees" />
				<field name="course_time" String="Course Time" />
				<field name="univercity_book_ids" invisible="1" String="Course Books" /> 
			</tree>
		</field>
	</record>
	<record id="view_univercity_course_form" model="ir.ui.view">
		<field name="name">univercitycourse.module.form</field>
		<field name="model">univercitycourse.module</field>
		<field name="arch" type="xml">
			<form string="Univercity Course" class="o_sale_order">
				<sheet>
					<group>
						<group>
							<field name="name" String="Course Name" />
							<field name="code" String="Course Code" />
						</group>
						<group>
							<field name="qualification_id" String="Qualification Required" />
							<field name="univercity_book_ids" widget="many2many_tags" options=
								"{'no_create_edit':True}" String="Course Books" />
						</group>
					</group>
					<group>
						<group>
							<field name="education_id" String="Education Stream"/>
							<field name="course_type" String="Course Type" />
						</group>
						<group>
							<field name="course_name" String="Course Full Name" />
							<field name="course_duration" String="Course Duration" />
						</group>
					</group>
					<group>
						<group>
							<field name="course_fees" String="Course Fees" />
						</group>
						<group>
							<field name="course_time" String="Course Time" />
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>
	<record id="univercity_course_action" model="ir.actions.act_window">
		<field name="name">Univercity Course</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">univercitycourse.module</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
		<field name="help" type="html">
			<p class="oe_view_nocontent_create">
			</p><p>
			</p>
		</field>
	</record>

	<menuitem action="univercity_course_action"
			id="menu_sample_module_5" parent="Univercity.menu_univercity_application_config"
			name="Univercity Course" sequence="7" />
</odoo>